{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {"Name": "Console"},
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://vm-igmo-dev.dobroga.local:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "ShippingDocuments",
      "Environment": "Development"
    }
  },
  "RabbitMq": {
    "IsUse": true,
    "HostName": "vm-igmo-dev.dobroga.local",
    "UserName": "RabbitMqUser",
    "Password": "Pwd4RabbitMq",
    "QueueBinds": [
      {
        "Exchange": "whs",
        "Queue": "mngr-order-out",
        "RoutingKey": "mngr-order-out"
      }
    ]
  }
}
